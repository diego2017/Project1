<% if @current_user %>
  <h4>Welcome <%= @current_user.name %>!</h4>
<%end%>
<h1> Cool places in Sydney </h1>

<button class="ui button"><%= link_to "Add a cool place", new_place_path  %></button>

<ul>
  <% @places.each do |place| %>
    <li>
      <%= link_to place.name, place_path(place.id) %>
    </li>
  <%end%>
</ul>
